{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "The Id Schema ",
      "default": 0,
      "examples": [
        1
      ]
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": [
        "Resident Evil 4"
      ]
    },
    "releaseDate": {
      "$id": "/properties/releaseDate",
      "type": "string",
      "title": "The Releasedate Schema ",
      "default": "",
      "examples": [
        "2005-10-01"
      ]
    },
    "reviewScore": {
      "$id": "/properties/reviewScore",
      "type": "integer",
      "title": "The Reviewscore Schema ",
      "default": 0,
      "examples": [
        85
      ]
    },
    "category": {
      "$id": "/properties/category",
      "type": "string",
      "title": "The Category Schema ",
      "default": "",
      "examples": [
        "Shooter"
      ]
    },
    "rating": {
      "$id": "/properties/rating",
      "type": "string",
      "title": "The Rating Schema ",
      "default": "",
      "examples": [
        "Universal"
      ]
    }
  }
}